{
  "name": "monorepo",
  "version": "1.0.0",
  "scripts": {
    "prepare": "husky",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "build": "turbo run build",
    "start": "cd website && next start",
    "documentation": "npx tsx scripts/documentation.ts",
    "publish": "pnpm build && npm publish --workspace=packages/cambio"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "turbo": "^2.5.4",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.5.0"
}
